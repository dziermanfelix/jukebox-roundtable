    "babel-jest": "^29.7.0",
    "babel-preset-vite": "^1.1.3",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react",
      "babel-preset-vite"
    ],
    "plugins": []
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
