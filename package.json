    "nodemon": "^3.1.7",
    "set-cookie-parser": "^2.7.1",
    "socket.io": "^4.8.1"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "node main.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "test": "jest --runInBand",
