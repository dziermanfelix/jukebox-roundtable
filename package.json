  "dependencies": {
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "concurrently": "^9.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "http": "^0.0.1-security",
